<h1>Your list of addresses</h1>

@foreach(var address in Model)
{
  <li>@address.getName()</li>
  <li>@address.getAddress()</li>
  <li>@address.getPhone()</li>
  <a href="/deleteOne/@address.getId()">Remove Me</a>
</br>
}

<p><a href = "/">Home</a></p>
<p><a href = "/add">Add a new contact</a></p>
<p><a href = "/deleteAll">Clear contact list</a></p>
